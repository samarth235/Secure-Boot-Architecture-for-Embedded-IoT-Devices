:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x104dc2850 .scope module, "tb_secure_boot" "tb_secure_boot" 2 1;
 .timescale 0 0;
v0x104dc71c0_0 .net "boot_fail", 0 0, L_0x104dc5a00;  1 drivers
v0x104dc7260_0 .net "boot_ok", 0 0, v0x104dc07d0_0;  1 drivers
v0x104dc7d40_0 .var "clk", 0 0;
v0x104dc7de0_0 .var "power_on", 0 0;
S_0x104dc29d0 .scope module, "uut" "secure_boot" 2 8, 3 1 0, S_0x104dc2850;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "power_on";
    .port_info 2 /OUTPUT 1 "boot_ok";
    .port_info 3 /OUTPUT 1 "boot_fail";
L_0x104dc7300 .functor AND 1, L_0x104dc7e80, v0x104dc7de0_0, C4<1>, C4<1>;
L_0x104dc5a00 .functor NOT 1, L_0x104dc7e80, C4<0>, C4<0>, C4<0>;
v0x104dc5c40_0 .net "boot_condition", 0 0, L_0x104dc7300;  1 drivers
v0x104dc5960_0 .net "boot_fail", 0 0, L_0x104dc5a00;  alias, 1 drivers
v0x104dc07d0_0 .var "boot_ok", 0 0;
L_0x938854010 .functor BUFT 1, C4<10101010>, C4<0>, C4<0>, C4<0>;
v0x104dc0870_0 .net "boot_rom", 7 0, L_0x938854010;  1 drivers
v0x104dc0910_0 .net "clk", 0 0, v0x104dc7d40_0;  1 drivers
L_0x938854058 .functor BUFT 1, C4<11110000>, C4<0>, C4<0>, C4<0>;
v0x104dc09b0_0 .net "firmware_rom", 7 0, L_0x938854058;  1 drivers
v0x104dc0a50_0 .net "firmware_valid", 0 0, L_0x104dc7e80;  1 drivers
v0x104dc7120_0 .net "power_on", 0 0, v0x104dc7de0_0;  1 drivers
E_0x104dc66d0 .event posedge, v0x104dc0910_0;
L_0x104dc7e80 .cmp/eq 8, L_0x938854010, L_0x938854058;
    .scope S_0x104dc29d0;
T_0 ;
    %wait E_0x104dc66d0;
    %load/vec4 v0x104dc5c40_0;
    %assign/vec4 v0x104dc07d0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x104dc2850;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x104dc7d40_0;
    %inv;
    %store/vec4 v0x104dc7d40_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x104dc2850;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x104dc7d40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x104dc7de0_0, 0, 1;
    %vpi_call 2 22 "$display", "Time\011Power\011Boot_OK\011Boot_Fail" {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x104dc7de0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 26 "$display", "%0t\011%b\011%b\011%b", $time, v0x104dc7de0_0, v0x104dc7260_0, v0x104dc71c0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 27 "$display", "%0t\011%b\011%b\011%b", $time, v0x104dc7de0_0, v0x104dc7260_0, v0x104dc71c0_0 {0 0 0};
    %delay 20, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_2;
